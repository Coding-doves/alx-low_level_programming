#include "main.h"

/**
 * jack_bauer - prints 24hours
 * 
 * Return: time
 */
void jack_bauer(void)
{
int h, m;

for (h = 0; h <= 23; h++)
{
for (m = h + 0; m <= 60; m++)
{
_putchar((h / 10) + '0');
_putchar((h % 10) + '0');
_putchar(':');
_putchar((m / 10) + '0');
_putchar((m % 10) + '0');
if ((h == 23) && (m == 60))
{
continue;
}
_putchar('\n');
}
}
_putchar('\n');
return (0);
}
